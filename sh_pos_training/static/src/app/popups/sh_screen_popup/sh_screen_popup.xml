<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
        <t t-name="sh_pos_training.custom_popup">
            <Dialog title="props.title">
                <div>Dhaval</div>
                <t t-esc="console.log('1234567', all_order_note)"/>
                <t t-foreach="all_order_note" t-as="note" t-key="note.id">
                <button t-on-click="() => this.set_order_note(all_order_note[note].name)" type="button" class="btn btn-lg me-2 mb-2 toggle-button lh-lg" >

                    <t t-esc="all_order_note[note].name"></t>

                </button>
            </t>
            </Dialog>
        </t>
</templates>