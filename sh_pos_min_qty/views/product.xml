<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_template_form_view_inherit_sh_pos_min_qty" model="ir.ui.view">
        <field name="name">product.inherit.view.form.inherit</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//div[@name='list_price_uom']" position="after">
                <field name="sh_min_qty"></field>
                <field name="sh_res_user_ids"  widget="many2many_tags"/>
            </xpath>
        </field>
    </record>

</odoo>