<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="th_pos_training.sh_payment_pop_up">
        <Dialog title="'Payment screen pop up'">
            <div>Taha</div>
            <lable for="payment_input">payment Info</lable>
            <input id="payment_input" type="text"/>
            <t t-set-slot="footer">
                <div class="modal-footer-left d-flex gap-2">
                    <div class="modal-footer-left d-flex gap-2">
                        <button class="button highlight btn btn btn-primary" t-on-click="confirm">Confirm</button>
                        <button class="button btn btn btn-secondary" t-on-click="close">Close</button>
                    </div>
                </div>
            </t>
        </Dialog>
    </t>
</templates>