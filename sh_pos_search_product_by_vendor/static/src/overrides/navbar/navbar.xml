<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
  <t t-name="sh_logo_replace.Navbar" t-inherit="point_of_sale.Navbar" t-inherit-mode="extension">
    <xpath expr="//div[hasclass('pos-centerheader')]" position="before">
      <div class="w-auto">
        <Input tModel="[pos, 'searchProductByVendor']"
        class="'p-0'"
        placeholder.translate="Search By Vendor..."
        icon="{type: 'fa', value: 'fa-search fa-lg fa-fw'}"
        />
      </div>         
    </xpath>
  </t>
</templates>